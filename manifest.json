{
    "name": "Common Cube Chrome Extension",
    "version": "0.1",
    "description": "Chrome extension that replaces images on mtgadraft.tk with custom images. Functionally allowing players to draft custom cards.",
    "manifest_version": 2,
    "icons": {
      "16": "assets/images/icon16.png",
      "32": "assets/images/icon32.png",
      "48": "assets/images/icon48.png",
      "128": "assets/images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://mtgadraft.tk/*"],
        "all_frames": true,
        "js": ["assets/js/contentScript.js"]
    }],
    "permissions": ["https://github.com/pat-norris/common-cube/blob/main/Common_Cube_Custom_Set_Cockatrice_Pics/*"]
}